{
  "name": "@red-scoop/nestjs-captcha",
  "version": "1.0.1-c",
  "description": "Recaptcha/HCaptcha module for NestJS.",
  "keywords": [
    "nestjs",
    "captcha",
    "google recaptcha",
    "hcaptcha",
    "nestjs recaptcha",
    "nestjs hcaptcha"
  ],
  "author": "Danil Bayer",
  "private": false,
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc && cp package.json dist && cp README.md dist && cp LICENSE dist",
    "format": "prettier \"**/*.ts\" \"**/*.json\" --ignore-path ./.prettierignore --write",
    "test": "jest --silent=false",
    "test:cov": "jest --coverage --coverageReporters=\"json-summary\"",
    "publish-package": "npm run build && npm publish --access public"
  },
  "dependencies": {
    "@nestjs/core": "^10.4.12",
    "axios": "^1.7.8"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.4.12",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Red-Scoop/nestjs-captcha.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Red-Scoop/nestjs-captcha/issues"
  },
  "homepage": "https://github.com/Red-Scoop/nestjs-captcha#readme"
}
